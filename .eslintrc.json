{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off"
    },
    "ignorePatterns": [
        "dist",
        "node_modules",
        "webpack.common.js",
        "webpack.dev.js",
        "webpack.prod.js",
        "postcss.config.js"
    ]
}
